<?php

 /** 
 * Implements hook_update_7001()
 *
 * Clean up School External ID Data.
 */
function eddy_school_sync_update_7001() {
	
	db_query("DELETE {field_revision_field_external_id} WHERE entity_type = 'node' and bundle = 'school' and NOT EXISTS(SELECT NULL FROM {node} n WHERE entity_id = n.nid)")->execute();

	return t('School External Id Data has been cleaned.');
	
}