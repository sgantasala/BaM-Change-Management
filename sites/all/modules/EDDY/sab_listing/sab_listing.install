<?php

/** 
 * Change module weight to have presidence over others.
 */
function sab_listing_update_7001() {
    
    db_update('system')
	->fields(array('weight' => 1))
	->condition('name', 'sab_listing')
    ->condition('type', 'module')
	->execute();

    return t('SAB Listings module weight has changed to 1.');

}